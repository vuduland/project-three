{
  "name": "trashtag",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "proxy": "http://localhost:5000",
  "scripts": {
    "start": "if-env NODE_ENV=production && yarn run prod || yarn run dev",
    "prod": "node server.js",
    "dev": "concurrently \"nodemon --ignore 'client/*'\" \"yarn run client\"",
    "server": "nodemon server.js",
    "client": "cd client && yarn run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && yarn",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Delia Bouhan, Jay Smith, Peter Smith, & Victor Uland",
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^5.3.1",
    "google-maps-react": "^2.0.2",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "materialize-css": "^1.0.0-rc.2",
    "mongoose": "^5.7.5",
    "npm": "^6.14.4",
    "react-materialize": "3.5.9"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.1"
  }
}
